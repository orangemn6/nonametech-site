#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/css-selector-tokenizer@0.7.3/node_modules/css-selector-tokenizer/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/css-selector-tokenizer@0.7.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
