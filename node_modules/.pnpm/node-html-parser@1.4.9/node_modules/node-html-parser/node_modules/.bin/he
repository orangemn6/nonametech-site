#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/he@1.2.0/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-html-parser@1.4.9/node_modules/node-html-parser/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-html-parser@1.4.9/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
