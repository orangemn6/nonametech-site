#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/mime@2.5.2/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/webpack-dev-middleware@3.6.0_webpack@4.29.0/node_modules/webpack-dev-middleware/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/webpack-dev-middleware@3.6.0_webpack@4.29.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
fi
