#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/node-gyp@7.1.2/node_modules/node-gyp/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-gyp@7.1.2/node_modules/node-gyp/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-gyp@7.1.2/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-sass@5.0.0/node_modules/node-sass/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node-sass@5.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp@7.1.2/node_modules/node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp@7.1.2/node_modules/node-gyp/bin/node-gyp.js" "$@"
fi
