#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/mkdirp@0.5.1/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/recursive-copy@2.0.6/node_modules/recursive-copy/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/recursive-copy@2.0.6/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
fi
