#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/amphtml-validator@1.0.23/node_modules/amphtml-validator/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/amphtml-validator@1.0.23/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules/amphtml-validator/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../amphtml-validator/index.js" "$@"
else
  exec node  "$basedir/../amphtml-validator/index.js" "$@"
fi
