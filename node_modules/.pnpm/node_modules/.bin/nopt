#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/nopt@5.0.0/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules/nopt/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules/nopt/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
