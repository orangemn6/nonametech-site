#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jacob/nonametech-site/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/ansi-html@0.0.7/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/node_modules:/home/jacob/nonametech-site/node_modules:/home/jacob/node_modules:/home/node_modules:/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/webpack-hot-middleware@2.24.3/node_modules/webpack-hot-middleware/node_modules:/home/jacob/nonametech-site/node_modules/.pnpm/webpack-hot-middleware@2.24.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-html@0.0.7/node_modules/ansi-html/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../ansi-html@0.0.7/node_modules/ansi-html/bin/ansi-html" "$@"
fi
